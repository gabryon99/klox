class Math {
    static square(n) {
        return n * n;
    }

    instanceMethod(a, b) {
        return a + b;
    }
}

Math.pi = 3.14;

class Circle {
    init(radius) {
        this.radius = radius;
    }

    // Getter method JavaScript-like
    area {
        return Math.pi * Math.square(this.radius);
    }
}

var circle = Circle(10);
print circle.area;